<div class="falsh-messages">
    <%if (typeof flashMsgs !== 'undefined') { %>
        <div class="mt-4"">
            <% flashMsgs.forEach(function(flasMsg) { %>
                <div class="alert alert-<%= flasMsg.type %> alert-dismissible fade show" role="alert">
                    <%= flasMsg.msg %>
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
            <% }); %>
        </div>
    <% } %>
</div>
